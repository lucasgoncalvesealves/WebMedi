    <servlet-name>ConfirmarCancelamento</servlet-name>
    <url-pattern>/ConfirmarCancelamento.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ValidarDadosConsultaSaldo</servlet-name>
    <servlet-class>servlets.ValidarDadosConsultaSaldoServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ValidarDadosConsultaSaldo</servlet-name>
    <url-pattern>/ValidarDadosConsultaSaldo.do</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>jsp/index.jsp</welcome-file>
  </welcome-file-list>
  <error-page>
    <error-code>403</error-code>
    <location>/jsp/403.jsp</location>
  </error-page>
  <error-page>
    <error-code>404</error-code>
    <location>/jsp/404.jsp</location>
  </error-page>
</web-app>